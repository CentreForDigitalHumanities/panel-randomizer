{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "declaration": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitUseStrict": true,
        "noImplicitReturns": false,
        "outDir": "./panel_randomizer_app/static/scripts",
        "sourceMap": true,
        "strictNullChecks": false
    },
    "exclude": [
        "panel_randomizer_app/static/scripts",
        "node_modules",
        ".npm",
        "**/*.spec.ts"
    ]
}
